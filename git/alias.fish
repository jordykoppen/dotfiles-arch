alias ga="git add"
alias gc="git commit --verbose"
alias gd="git diff"
alias gs="git status"
alias gcm="git commit -m"
alias gfa="git fetch --all"
alias gco="git checkout"
alias gcom="gco master"
alias gcob="gco -b"
alias gpl="git pull"
alias gp="git push"
alias gpu="git push -u origin (git branch | grep * | cut -d ' ' -f2)"
alias gacop="ga .; gc; gp"
